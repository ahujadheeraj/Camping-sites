<% include ../partials/header %><div class="container">    <div class="row">        <div class="col-md-3">            <p class="lead">YelpCamp</p>            <div class="list-group">                <li class="list-group-item active">abc</li>                <li class="list-group-item">def</li>                <li class="list-group-item">ghi</li>            </div>        </div>        <div class="col-md-9">            <div class="thumbnail">                <img src="<%=campground.image%>">                <div class="caption">                    <% if(currentUser && campground.author.id.equals(currentUser._id)){  %>                    <a  class="btn btn-warning" href="/campgrounds/<%= campground._id%>/edit">Edit</a>                    <form id="delete_form" action="/campgrounds/<%=campground._id%>?_method=delete" method="post">                        <button class="btn btn-danger">Delete</button>                    </form>                    <% } %>                    <h4 class="pull-right"><%= campground.price %></h4>                    <h4> <%= campground.name %></h4>                    <p><%= campground.description %></p>                    <p>                        <em> submitted by : <%= campground.author.username %> </em>                    </p>                </div>            </div>            <div class="well">                <div class="text-right">                    <a class="btn btn-success" href="/campgrounds/<%=campground._id%>/comments/new">Add New Comment</a>                </div>                <% campground.comments.forEach(function(comment){ %>                <div class="row">                    <div class="col-md-12">                        <strong><%=comment.author.username%></strong>                        <span class="pull-right">10 days ago</span>                        <p>                            <%=comment.text%>                        </p>                        <% if(currentUser && comment.author.id.equals(currentUser._id)){  %>                        <a class="btn btn-xs btn-warning" href="/campgrounds/<%= campground._id %>/comments/<%= comment._id %>/edit">Edit Comment</a>                        <form id="delete_form" action="/campgrounds/<%= campground._id %>/comments/<%= comment._id %>?_method=DELETE" METHOD="POST">                            <input type="submit" value="Delete" class="btn btn-xs btn-danger">                        </form>                        <% } %>                    </div>                </div>                <% }); %>            </div>        </div>    </div></div><p></p><% include ../partials/footer %>